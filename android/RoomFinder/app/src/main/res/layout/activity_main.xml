<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a100541476.roomfinder.MainActivity">


    <LinearLayout
        android:layout_width="399dp"
        android:layout_height="678dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <Button
            android:id="@+id/quick_search"
            android:onClick="quickSearch"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:text="Quick Search" />

        <Button
            android:id="@+id/advanced_search"
            android:onClick="advancedSearch"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:text="Advanced Search" />
        <Button
            android:id="@+id/room_sched"
            android:onClick="searchRoom"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:text="Search Room Schedule" />
    </LinearLayout>


</android.support.constraint.ConstraintLayout>
